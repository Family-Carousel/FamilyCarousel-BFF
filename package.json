{
  "name": "familycarousel-bff",
  "version": "1.0.0",
  "description": "Backend API for the front end UI running graphQL",
  "main": "handlers/handler.js",
  "scripts": {
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "start": "yarn serverless offline --noTimeout",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline",
    "deploy:dev": "serverless deploy -s dev",
    "deploy": "serverless deploy -s prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Family-Carousel/FamilyCarousel-BFF.git"
  },
  "author": "Michael Cavanaugh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Family-Carousel/FamilyCarousel-BFF/issues"
  },
  "homepage": "https://github.com/Family-Carousel/FamilyCarousel-BFF#readme",
  "dependencies": {
    "apollo-datasource-rest": "^0.6.6",
    "apollo-server-lambda": "^2.9.6",
    "axios": "^0.19.0",
    "config": "^3.2.3",
    "graphql": "^14.5.8",
    "graphql-depth-limit": "^1.1.0"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "serverless": "^1.54.0",
    "serverless-domain-manager": "^3.3.0",
    "serverless-offline": "^5.12.0",
    "serverless-plugin-optimize": "^4.1.0-rc.1",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.41.1",
    "webpack-node-externals": "^1.7.2"
  },
  "graphdoc": {
    "endpoint": "http://localhost:3000/graphql",
    "output": "./doc"
  }
}
